doctype html
html(lang="en-US")
    head
        meta(charset='utf-8')
        meta(http-equiv='X-UA-Compatible', content='IE=edge, chrome=1')
        title Fiverr open source software
        meta(name='viewport', content='width=device-width, initial-scale=1')
        meta(name='description', content='Fiverr engineering open source software used to build our products.')
        link(rel='shortcut icon', href='https://www.fiverr.com/favicon.ico')
        link(rel='author', href='https://github.com/fiverr/fiverr.github.io')
    body
        header
            a(href='https://www.fiverr.com', rel='noreferrer moopener')
                img(src='./images/fiverr.svg', alt='Fiverr logo')
            h1 Open Source
        main
        aside
        footer
        script(src='./index.js')
